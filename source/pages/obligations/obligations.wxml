<!--pages/obligations/obligations.wxml-->
<view class="text-center" style="height:150rpx;">请尽快付款</view>

<view   class="margin-20">


  <view class="flex-row flex-center padding-20" style="border-bottom:2rpx solid #BBBBBB">
    <view class="flex-1">订单号</view>
    <view>{{detail.orderno}}</view>
  </view>

  <view class="flex-row flex-center padding-20" style="border-bottom:2rpx solid #BBBBBB">
    <view class="flex-1">在线支付</view>
    <view>￥{{detail.totalamount}}</view>
  </view>

  <view class="flex-row flex-center padding-20" >
    <view class="flex-1">供应商</view>
    <view>{{detail.enterprise_id_name}}</view>
  </view>
<view style="height:50rpx;"></view>
  

</view>

<view class="bg-white bott">
  <view class="flex-row margin-top-20 padding-bottom-20">
    <view class="flex-1"></view>
    <view class="txt-black margin-top-20">应付：￥{{detail.totalamount}}</view>
    <view class="bgjie txt-white margin-left-20 margin-right-20 text-center" id="{{detail.totalamount}}" bindtap="bindbuy">购买</view>
  </view>
</view>