<view class='bg-white '>
  <view class=' flex-row '>
    <view class='btnn buttonone'>
      <input class=' margin-left-20' placeholder='请输入要查询的配件' value="{{searchinput}}" bindconfirm="binpartsearch" bindinput="binpart"></input>
      <view class="flex-1"></view>
      <view class='flex-row flex-wrap '>
        <image class='icon-17 margin-right-20' src='{{uploadpath}}resource/{{res.delect}}' bindtap='binddelete'></image>
      </view>
    </view>

  </view>
</view>


<block wx:for="{{partsearch}}" wx:key="k">
  <view class='txt-black margin-top-40 margin-left-30 margin-bottom-20'>{{item.brandCode}}</view>
  <view class='bott margin-bottom-20'></view>
</block>

<view class='btn bg-btn ' bindtap='addclick'>确认该配件</view>