<view class='bg-white '>
  <view class=' flex-row '>
    <view class='btnn buttonone'>
      <input class=' margin-left-20' placeholder='请输入要查询的配件' value="{{searchinput}}" bindconfirm="binpart" bindinput="binpart"></input>
      <view class="flex-1"></view>
      <view class='flex-row flex-wrap '>
        <image class='icon-17 margin-right-20' src='{{uploadpath}}resource/{{res.delect}}' bindtap='binddelete'></image>
      </view>
    </view>

  </view>
</view>


<block wx:for="{{partsearch.data}}" wx:key="k">
  <view class='txt-black margin-top-40 margin-left-30 margin-bottom-20' data-name='{{item.struct_label}}' bindtap="bindlabel">{{item.pid}}{{item.name}}</view>
  <view class='bott margin-bottom-20'></view>
</block>

<view class='vbtn' bindtap='addclick'>+手动添加</view>