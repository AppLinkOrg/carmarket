<view class="bg-btn ">
  <view class="flex-row margin-left-10 margin-bottom-10">
    <image class='icon-90 radius-50%;' src='{{uploadpath}}enterprise/{{employeeinfo.enterprise.logo}}'></image>
    <view class=" margin-top-60  ">
      <view class="txt-white margin-left-20">{{employeeinfo.name}}</view>
      <view class="txt-white flex-row  margin-left-20">{{employeeinfo.enterprise.name}}</view>
    </view>
    <view class='flex-1'></view>
    <image class='icon-17 margin-right-40 margin-top-80 ' bindtap='bindright' src='{{uploadpath}}resource/{{res.whiteright}}'></image>
  </view>
</view>

<view class=' flex-row margin-top-30 '>
  <view class='borderleft'></view>
  <view class='margin-left-10 txt-black txt-bold'>我的账户</view>
</view>
<view class='flex-row margin-top-30'>
  <view class='margin-top-10 margin-left-50 h3 txt-orange txt-bold'>{{employeeinfo.enterprise.vdollar}}</view>
  <view class='h5 txt-black margin-left-30 margin-top-20'>威币</view>
  <view class='flex-1'></view>
  <view class='bg-btn  gbtn margin-right-10'>额度申请</view>
</view>
<view class='h5 txt-black margin-left-50  '>账户余额</view>
<view class='bott margin-top-20'></view>

<view class="flex-wrap  flex-center ">

  <view class='flex-row xia'>
    <view class="mar">
      <image class='icon ' src='{{uploadpath}}resource/{{res.order}}'></image>
    </view>
    <view class=' mar txt-black txt-bold'>我的订单</view>
    <view class='flex-1'></view>
    <image class='icon-17 margin-right-20 margin-top' src='{{uploadpath}}resource/{{res.smallright}}' bindtap="bindorder"></image>
  </view>

  <view class='flex-row xia'>
    <view class="mar">
      <image class='icon ' src='{{uploadpath}}resource/{{res.address}}'></image>
    </view>
    <view class=' mar txt-black txt-bold'>我的地址</view>
    <view class='flex-1'></view>
    <image class='icon-17 margin-right-20 margin-top' src='{{uploadpath}}resource/{{res.smallright}}' bindtap="bindaddress"></image>
  </view>


  <view url="/pages/authority/authority" class='flex-row xia'>
    <view class="mar">
      <image class='icon ' src='{{uploadpath}}resource/{{res.authority}}'></image>
    </view>
    <view class=' mar txt-black txt-bold'>我的权限</view>
    <view class='flex-1'></view>
    <image class='icon-17 margin-right-20 margin-top' src='{{uploadpath}}resource/{{res.smallright}}' bindtap="bindauth"></image>
  </view>


  <view url="/pages/account/account" class='flex-row xia'>
    <view class="mar">
      <image class='icon ' src='{{uploadpath}}resource/{{res.account}}'></image>
    </view>
    <view class=' mar txt-black txt-bold'>我的账户</view>
    <view class='flex-1'></view>
    <image class='icon-17 margin-right-20 margin-top' src='{{uploadpath}}resource/{{res.smallright}}' bindtap="bindaccount"></image>
  </view>

  <view url="/pages/change/change" class='flex-row  xia'>
    <view class="mar">
      <image class='icon' src='{{uploadpath}}resource/{{res.change}}'></image>
    </view>
    <view class=' mar txt-black txt-bold'>退换货</view>
    <view class='flex-1'></view>
    <image class='icon-17 margin-right-20 margin-top' src='{{uploadpath}}resource/{{res.smallright}}' bindtap="bindchange"></image>
  </view>

  <view url="/pages/aboutus/aboutus" class='flex-row  xia'>
    <view class="mar">
      <image class='icon' src='{{uploadpath}}resource/{{res.about}}'></image>
    </view>
    <view class=' mar txt-black txt-bold'>关于我们</view>
    <view class='flex-1'></view>
    <image class='icon-17 margin-right-20 margin-top' src='{{uploadpath}}resource/{{res.smallright}}' bindtap="bindaboutus"></image>
  </view>

  <view url="/pages/helpcenter/helpcenter" class='flex-row  xia'>
    <view class="mar">
      <image class='icon' src='{{uploadpath}}resource/{{res.help}}'></image>
    </view>
    <view class=' mar txt-black txt-bold'>帮助反馈</view>
    <view class='flex-1'></view>
    <image class='icon-17 margin-right-20 margin-top' src='{{uploadpath}}resource/{{res.smallright}}' bindtap="bindhelp"></image>
  </view>

  <view class='flex-row  xia '>
    <view class="mar">
      <image class='icon' src='{{uploadpath}}resource/{{res.outlogin}}'></image>
    </view>
    <view class=' mar txt-black txt-bold'>退出登录</view>
    <view class='flex-1'></view>
    <image class='icon-17 margin-right-20 margin-top' src='{{uploadpath}}resource/{{res.smallright}}' bindtap="logout"></image>
  </view>
</view>