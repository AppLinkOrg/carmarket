Page {
  background: rgb(241, 241, 241);
}

.borderbottom {
  border-bottom: 1px solid rgb(233, 233, 233);
}

.btn {
  background-color: #e92a11;
  width: 200rpx;
  height: 80rpx;
  border-radius: 40rpx;
}

.button {
  bottom: 0;
  position: fixed;
  width: 100vw;
  background-color: white;
}

.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  background: #000;
  opacity: 0.7;
}

.dialog-box {
  width: 100%;
  height: 620rpx;
  position: fixed;
  bottom: 0;
  background-color: rgb(255, 255, 255);
  border-radius: 36rpx 36rpx 0rpx 0rpx;
}

.qw {
  background-color: #e51c23;
  margin: 20rpx;
  height: 100rpx;
  width: 92%;
  color: white;
}
